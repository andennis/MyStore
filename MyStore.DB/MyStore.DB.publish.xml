<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="14.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <IncludeCompositeObjects>True</IncludeCompositeObjects>
    <TargetDatabaseName>MyStore</TargetDatabaseName>
    <DeployScriptFileName>MyStore.DB.sql</DeployScriptFileName>
    <TargetConnectionString>Data Source=localhost;Persist Security Info=True;User ID=sa;Pooling=False</TargetConnectionString>
    <ProfileVersionNumber>1</ProfileVersionNumber>
    <BlockWhenDriftDetected>False</BlockWhenDriftDetected>
    <RegisterDataTierApplication>False</RegisterDataTierApplication>
    <BlockOnPossibleDataLoss>False</BlockOnPossibleDataLoss>
  </PropertyGroup>
</Project>